{
  "name": "healmate",
  "version": "1.0.0",
  "description": "HealMate Healthcare Application - Complete Healthcare Management System",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../patient-healthcare && npm install",
    "start-backend": "cd backend && npm start",
    "start-frontend": "cd patient-healthcare && npm run dev",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "build": "cd patient-healthcare && npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "healthcare",
    "react",
    "nodejs",
    "express",
    "typescript",
    "medical",
    "appointment",
    "patient"
  ],
  "author": "HealMate Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/healmate.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
} 